{
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/",
      "destination": "/security",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/security",
      "destination": "/secure.html"
    },
    {
      "source": "/id",
      "destination": "/index.html"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "/404.html" }
  ]
}
